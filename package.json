{
  "name": "mini-program-polyfill",
  "version": "1.0.1",
  "private": false,
  "description": "小程序 mini-program polyfill",
  "main": "index.js",
  "files": [
    "README.md"
  ],
  "scripts": {
    "eslint:fix": "eslint --config ./.eslintrc.js --fix --ext .js,.ts ./",
    "prettier": "prettier --config ./.prettierrc.js --write **/*.{js,ts}",
    "test": "jest",
    "publish": "pnpm publish"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "npm run prettier",
      "npm run eslint:fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -x @commitlint/config-conventional -e"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@types/express": "^4.17.13",
    "@types/express-formidable": "^1.2.0",
    "@types/gulp": "^4.0.9",
    "@types/gulp-terser": "^1.2.1",
    "@types/inquirer": "^8.2.1",
    "@types/jest": "^27.4.1",
    "@types/rimraf": "^3.0.2",
    "@types/vinyl": "^2.0.6",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "babel-plugin-add-module-exports": "^1.0.4",
    "chalk": "^4.1.2",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "express": "^4.17.3",
    "express-formidable": "^1.2.0",
    "gulp": "^4.0.2",
    "gulp-terser": "^2.1.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "husky": "4",
    "inquirer": "^8.2.2",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.7",
    "minidev": "^1.3.1-rc.1",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "tslib": "^2.3.1",
    "tt-ide-cli": "^0.1.9",
    "typescript": "^4.5.0"
  },
  "keywords": [
    "mini-program",
    "wx",
    "支付宝",
    "微信",
    "小程序",
    "blob",
    "file",
    "form-data",
    "polyfill"
  ],
  "author": "zyrong",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "bugs": {
    "url": "https://github.com/zyrong/mini-program-polyfill/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zyrong/mini-program-polyfill"
  },
  "homepage": "https://github.com/zyrong/mini-program-polyfill/blob/master/README.md",
  "license": "MIT"
}
